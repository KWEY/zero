{
    "linterOptions": {
      "exclude": [
        "node_modules/**",
        "demo/**",
        "dist/**"
      ]
    },
    "extends": [
      "tslint-config-alloy"
    ],
    "rules": {
      "ter-indent": [
        true,
        4,
        {
            "SwitchCase": 1
        }
      ],
      "no-unnecessary-type-assertion": null,
      "no-for-in-array": null,
      "no-unbound-method": null,
      "return-undefined": null,
      "no-unused-variable": null,
      "no-use-before-declare": null,
      "deprecation": null,
      "prefer-const": true,
      "no-plusplus": null // ++
    },
    "rulesDirectory": [
      "node_modules/tslint-eslint-rules/dist/rules"
    ]
  }
  